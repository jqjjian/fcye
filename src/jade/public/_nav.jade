nav.navbar.navbar-default.navbar-fixed-top
  div.container
    div.navbar-header
      button.navbar-toggle.collapsed(type="button" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar")
        span.sr-only Toggle navigation
        - for(var i=0; i<3;i++){
          span.icon-bar
        - }
      a.navbar-brand(href="#")
        img.img-responsive(src="../static/images/logo_03.jpg")
    div#navbar.navbar-collapse.collapse
      ul.nav.navbar-nav.navbar-right
        - var menus = ['首页','新闻公告','活动剪影','公益慈善','青创会简介','服务介绍','会员之声','入会申请']
        - for(var i=0,len=menus.length;i<len;i++) {
            - if(i==0)
              li.active
                a(href="#")= menus[i]
            - else if(i==5)
              li.dropdown
                a.dropdown-toggle(data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false")= menus[i]
                  span.caret
                ul.dropdown-menu
                  - var subs = ['政务代理服务','创业孵化服务','管理咨询服务','城市会所服务','金融服务','会展会务服务','商机服务','人脉圈层服务','公关服务','国际商务服务','学习修炼服务','品牌战略服务','健康管理服务','夫人服务','子女服务','慈善公益服务']
                  - each sub in subs
                      li
                        a(href="#")= sub
            - else
              li
                a(href="#")= menus[i]
        - }